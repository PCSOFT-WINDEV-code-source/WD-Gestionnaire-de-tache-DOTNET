      // Valeur de retour :
      // 	Type indéterminé : 
      
      Procédure RécupPriorité(nIdProcess)
      
      sResultat est une chaine								// Le resultat que la fonction va renvoyer
      MonProcess est un "System.Diagnostics.Process" dynamique		// Objet .NET déclarant un processus
      MaPriorité est une processPriorityClass dynamique		// Objet .NET déclarant une classe de priorité des processus
      
