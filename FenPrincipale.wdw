   -
     name : RempliListeProcessus
     internal_properties : 
     procedure_id : 1542229301812629094
     type_code : 14
     code : |1-
      // Syntaxe :
      //RempliListeProcessus ()
      //	Cette Procédure sert à Lister tous les processus et leurs informations
      //  De plus, elle appelle de nombreuses méthodes et prorpiétés .NET en relation avec les Process, ProcessModule ou encore ProcessThreadCollection
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      Procédure RempliListeProcessus()
      
      // Variables
      Memoiretotale est un entier										// Compteur permettant de comptabiliser le total de memoire utilisée par les processus
      ListeProcessus est un tableau dynamique de 0 "System.Diagnostics.Process" dynamique	// déclaration d'un tableau d'objets .NET Process
      nbThread est un entier 											// Compteur du nombre de threads
      nbHandles est un entier											// Compteur du nombre de handles
      CollectionThread est un ProcessThreadCollection dynamique 		// Déclaration d'un objet .NET Collection de thread
      MonProcessModule est un ProcessModule dynamique 				// Déclaration d'un objet .NET ProcessModule
      pclMonProcess est un "System.Diagnostics.Process" dynamique
      sPriorité est une chaine										// Déclaration de la priorité
      nLigneProcess est un entier										// Numéro de la ligne sélectionnée dans la table process
      nLignePremierProcess est un entier								// Numéro de la première ligne affichée dans la table process
      nLigneProcess = TableSelect(tableprocess)						// Sauvegarde de la ligne sélectionnée
      sNomUser est une chaine											// Nom de l'utilisateur ayant créé le processus
      i est un entier = 1												// Compteur pour la boucle de remplissage des processus
      sNomProcess est une chaine										// Nom du processus
      
      // Sauvegarde de la 1 ere ligne affichée dans les tables
      nLignePremierProcess = TablePosition(TableProcess)
      
      
      // On rends les tables invisibles le temps du remplissage
      tableprocess..AffichageActif = faux
      
      // On supprime tout ce qu'il y a dans les tables
      TableSupprimeTout(Tableprocess)
      
      
      // Récupération de tous les processus par la méthode .NET GetProcesses
      ListeProcessus = process::GetProcesses()
      
      
      // Réinitialise le compteur
      i=1
      
      // Boucle tant qu'on ne dépasse pas la dimension du tableau retourné
      tantque i <= dimension(ListeProcessus)
      
      	pclMonProcess = ListeProcessus[i]
      	
      	// Récupération des threads associés au processus en cours grâce à la méthode .NET Threads
      	CollectionThread = pclMonProcess:Threads
      	// Mise a jour du nombre de threads associés au processus en cours grâce à la méthode .NET Count
      	nbThread += CollectionThread:Count
      	
      	// Mise a jour du nombre de handles associés au processus en cours : utilisation de la méthode .NET HandleCount
      	nbHandles += pclMonProcess:HandleCount
      	
      	// Selon le nom du processus récupéré par la méthode .NEt ProcessName
      	selon pclMonProcess:ProcessName 
      	
      		cas "Idle" // Processus inactif du systeme
      			// On change le nom du process pour avoir qqchose de plus agreable a lire
      			sNomProcess = <§$000d§>
      			// L'utilisateur ne peut pas être récupéré (acces refusé)
      			sNomUser = "SYSTEM"
      			sPriorité = <§$0012§>
      			
      		cas "System" // Processus systeme
      			sNomProcess = pclMonProcess:ProcessName
      			// L'utilisateur ne peut pas être récupéré (acces refusé)
      			sNomUser = "SYSTEM"
      			
      		cas "SVCHOST" // Processus SVCHOST
      			// Mise a jour du nom du processus
      			sNomProcess = "SVCHOST.EXE"
      			sNomUser = RecupUserName(pclMonProcess:id)
      			quand exception dans 
      				sPriorité = RécupPriorité(pclMonProcess:id)
      			faire
      				sPriorité = <§$0000§>
      			fin
      			
      		cas "CSRSS"
      			// Mise a jour du nom du processus
      			sNomProcess = pclMonProcess:ProcessName
      			// L'utilisateur ne peut pas être récupéré (acces refusé)
      			sNomUser = "SYSTEM"
      			sPriorité = <§$0001§>
      			
      		autres cas
      			
      			// Mise a jour du nom du processus
      			// Traitement des exceptions si le Process est protégé, il renvoie une exception
      			quand exception dans 
      				
      				// Appel de la méthode .NET MainModule qui renvoie un ProcessModule
      				MonProcessModule = pclMonProcess:MainModule
      				// On récupère le nom de l'executable principal lancé par le process grâce à la propriété .NET ModuleName
      				sNomProcess = MonProcessModule:ModuleName	
      				
      			faire
      				// Traitement de l'exception
      				// On met le nom du process et non le nom de l'executable
      				sNomProcess = pclMonProcess:ProcessName
      			FIN	
      				
      			sNomUser = RecupUserName(pclMonProcess:id)
      			SI sNomUser <> "" ALORS
      				sPriorité = RécupPriorité(pclMonProcess:id)
      			FIN	
      			
      	FIN
      	
      	// Ajoute dans la table des informations relatives au processus, Toutes les propriétés sont récupérées par les propriétés .NET correspondantes
      	TableAjouteLigne(TableProcess,pclMonProcess:id,sNomProcess,sNomUser,pclMonProcess:workingset / 1024,...
      	sPriorité,pclMonProcess:HandleCount,CollectionThread:count,pclMonProcess:PeakWorkingSet/1024)
      	
      	// Mise a jour de la memoire totale par l'appel à la propriété .NET WorkingSet
      	Memoiretotale+=pclMonProcess:WorkingSet
      	
      	
      	i++
      FIN
      
      // Déclaration de la variable de memoire
      mem est une MEMORYSTATUSEX
      // Déclaration de la variable de retour pour l'appel de l'API
      res est un entier
      // Appel de l' GlobalMemoryStatus qui permet de récupérer les informations mémoires du système
      res = API("Kernel32.dll","GlobalMemoryStatus",&mem)
      // Si le resultat est différent de 0, la structure a été remplie correctement
      si res <> 0 ALORS
      	// Donc on affiche les résultats
      	// Les valeurs sont en octets donc on divise par 1024 pour les avoir en Ko
      	LIB_MEMTOT = mem:ullTotalPhys/1024
      	LIB_MEMLIBRE = mem:ullAvailPhys/1024
      	
      	// La structure n'a pas été remplie correctementpour ces informations donc on n'affiche pas les informations
      	si mem:ullTotalPageFile = -1 ALORS
      		LIB_CHGDEDLIMITE=<§$0013§>
      		LIB_CHGDEDTOT=<§$0013§>
      	sinon
      		LIB_CHGDEDLIMITE = mem:ullTotalPageFile/1024
      		LIB_CHGDEDTOT = mem:ullTotalPageFile/1024 - mem:ullAvailPageFile/1024	
      	FIN
      	
      sinon
      	// La structure n'a pas été remplie correctement donc, on n'affiche pas les informations
      	LIB_MEMLIBRE="NC"
      	LIB_CHGDEDLIMITE=<§$0013§>
      	LIB_CHGDEDTOT=<§$0013§>
      	LIB_MEMTOT=<§$0013§>
      fin
      
      // Mise a jour des libellés des stats (nombre de processus, nombre de handles, nombre de threads)
      LIB_NB_PROCESSUS =  i - 1 
      LIB_NB_HANDLES = nbHandles
      LIB_NB_THREAD = nbThread
      
      // On trie la table des process selon l'ordre de tri défini (s'il y en a un)
      si striprocess<>"" ALORS
      	TableTrie(tableprocess,striprocess)
      fin
      
      // On sélectionne la ligne qui était sélectionnée avant le remplissage
      tableselectplus(tableprocess,nLigneProcess)
      // On remet la table dans la même position
      TablePosition(TableProcess,nLignePremierProcess)
      
      // On rend les tables des processus visibles
      tableprocess..AffichageActif = vrai
