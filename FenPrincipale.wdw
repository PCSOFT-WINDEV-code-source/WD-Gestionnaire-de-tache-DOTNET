     name : RecupUserName
     internal_properties : 
     procedure_id : 1542229301812694630
     type_code : 14
     code : |1+
      // Syntaxe :
      //[ <Résultat> = ] RecupUserName (<nPID> est entier)
      //
      // Paramètres :
      //	nPID (entier) : Identifiant du process dont il faut récupérer l'utilisateur
      
      // Valeur de retour
      // chaine : le nom d'utilisateur
      Procédure RecupUserName(nPID est un entier )
      
      SID_AND_ATTRIBUTES est une structure
      	pSID est un entier sur 4 octets
      	dwAttribut est un entier sur 4 octets
      FIN
      
      // Déclaration des variables paramètres
      PROCESS_QUERY_INFORMATION est un entier = 0x400
      hProcess est un entier sur 4 octets = 0x00
      TOKEN_QUERY est un entier sur 4 octets = 0x0008
      
      // déclaration des variables
      hToken est un entier sur 4 octets  = 0
      bRes est un booleen										// Variable contenant la valeur de retour de l'api
      dwTailleGroups est un entier sur 4 octets = 0			// Taille de la structure
      szNom est une chaine asciiz sur 250 = ""					// Nom d'utilisateur
      szDomaine est une chaine asciiz sur 250 = ""				// Domaine dont il fait partie
      dwTailleNom est un entier sur 4 octets = 250			// Taille de la chaine du nom
      dwTailleDomaine est un entier sur 4 octets = 250		// Taille de la chaine du Domaine
      eUse est un entier sur 4 octets = 0						// Type de compte que l'utilisateur possède
      pBuffer est un entier système					// Buffer de transfert entre les Apis et l'application
      
      stUser est un SID_AND_ATTRIBUTES
      // SI la version de Windows n'est pas sur une base NT, on renvoie vide
      // car les fonctions utilisées ne sont définies que sous un noyau NT
      si SysVersionWindows(sysVersionPlateForme) <> "NT" ALORS
      	renvoyer ""
      FIN
      
      
      // Appel de l'api pour ouvrir le process
      hProcess = API("kernel32","OpenProcess",PROCESS_QUERY_INFORMATION,0,nPID)
      
      // Ouverture du token du process
      
      bRes = API("advapi32","OpenProcessToken",hProcess,TOKEN_QUERY,&hToken)
      
      // Demande de la taille des infos de groupe
      API("advapi32","GetTokenInformation",hToken,1,NULL,0,&dwTailleGroups)
      
      // Allocation de la structure
      pBuffer = pMalloc(dwTailleGroups)
      
      // Récupération du SID_AND_ATTRIBUTES qui contient un pointeur vers le psid
      API("advapi32","GetTokenInformation",hToken,1,pBuffer,dwTailleGroups,&dwTailleGroups)
      
      // récupération de la valeur du psid à partir de son pointeur
      Transfert(&stUser,pBuffer,dimension(stUser))
      
      // récupération des informations de l'utilisateur
      API("advapi32","LookupAccountSidA",null,stUser:pSID,&szNom,&dwTailleNom,&szDomaine,&dwTailleDomaine,&eUse)
      
      renvoyer szNom
      	
