              code : |1+
               
               
               nLigne est un entier = TableInfoXY(MoiMême,tiNumLigne,SourisPosX(),SourisPosY())
               // Si la ligne est vide, on grise le menu
               
               si nLigne = -1 ALORS
               	menuappli.Agrandir..etat = grisé
               	Menuappli.Aller_dans_le_processus..etat = grisé
               	menuappli.BasculeVers..etat = grisé
               	menuappli.Fin_de_tâche..etat = grisé
               	MenuAppli.Mettre_au_premier_plan..etat = grisé
               	menuappli.Reduire..etat = grisé
               sinon // Sinon on le remet dans son etat initial
               	menuappli.Agrandir..etat = actif
               	Menuappli.Aller_dans_le_processus..etat = actif
               	menuappli.BasculeVers..etat = actif
               	menuappli.Fin_de_tâche..etat = actif
               	MenuAppli.Mettre_au_premier_plan..etat = actif
               	menuappli.Reduire..etat = actif
               FIN
               
               // Sélection de la ligne sur laquelle l'utilisateur a cliqué
               TableSelectPlus(MoiMême,nLigne)
               
               
