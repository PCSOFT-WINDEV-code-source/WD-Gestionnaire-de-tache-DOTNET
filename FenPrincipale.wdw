     code : |1-
      // Syntaxe :
      //RafraichiListeApplis ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      Procédure RafraichiListeApplis()
      
      nLigneSelect est un entier										// Nom de l'appli sélectionnée
      i est un entier = 1												// Compteur pour la boucle de remplissage des processus
      nLignePremierAppli est un entier								// Numéro de la première ligne affichée dans la table appli
      
      //Arrêt du timer
      fintimer(nNumTimerAppli)
      
      
      // Vérification de la ligne sélectionnée
      nLigneSelect = TableAppli
      
      // Sauvegarde de la premiere ligne affichée
      nLignePremierAppli = TablePosition(Tableappli)
      
      // On rends les tables invisibles le temps du remplissage
      TableAppli..AffichageActif = Faux
      // on remet le check de toutes les applis à 0
      tantque i <= tableoccurrence(tableappli)
      	TableAppli.Checked[i] = 0
      	i++
      FIN
      i=1
      // On appelle l'API EnumWindows qui enumère toutes les fenêtres
      api("USER32.dll","EnumWindows",&EnumèreFenêtres,0)
      
      // Etant donné que l'on a modifié ou ajouté les lignes de la table des applications, 
      // il faut vérifier que toutes les lignes affichées sont bien valides
      tantque i <= TableOccurrence(tableappli)
      	
      	si TableAppli.Checked[i] = faux ALORS
      		TableSupprime(TableAppli,i)
      	sinon 
      		i++
      	FIN
      FIN
      
      // On remet la table des applications dans la même position
      TablePosition(TableAppli,nLignePremierAppli)
      
      // On trie la table des applications selon l'ordre de tri défini (s'il y en a un)
      si sTriAppli <> "" ALORS
      	tabletrie(tableappli,striappli)
      FIN
      
      // Si elle n'est pas trouvée, c'est qu'elle a été quittée, donc on sélectionne la première
      si  nLigneSelect<> -1 alors
      	tableselectplus(tableAppli,nLigneSelect)
      fin
      // On remet la table en visible
      TableAppli..AffichageActif = vrai
      
      // relance le timer
      nNumTimerAppli = timer(RafraichiListeApplis,50)
