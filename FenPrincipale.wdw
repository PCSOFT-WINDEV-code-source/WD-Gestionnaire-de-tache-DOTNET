     code : |1+
      // Syntaxe :
      //[ <Résultat> = ] EnumèreFenêtres (<nhwnd> est entier système, <nLparam> est entier)
      //
      // Paramètres :
      //	nhwnd (entier système) : handle de la fenêtre trouvée
      //	nLparam (entier) : 0
      // Valeur de retour :
      // 	booléen :     renvoie vrai pour que l'"numération des fenêtres continue
      
      PROCEDURE EnumèreFenêtres(nhwnd est un entier système, nLparam est un entier <utile>)
      
      sTitre est une chaine 						// Titre de la fenetre
      MonProcess est un "System.Diagnostics.Process" dynamique // Déclaration de l'objet .NET qui va recevoir le process associé à la fenêtre
      nLigne est un entier						// Numéro de la ligne de l'application
      nProcessID est un entier					// Déclaration du Numéro de process
      sNomExe est une chaine						// Nom de l'executable 
      MonModule est un processModule dynamique 	// Module principal du processus concerné
      nHndIcone est un entier						// Handle de l'icone de l'application
      sFichierTmp est une chaine  				// Fichier temporaire ou sera stocké l'icone de l'executable
      rRectangle est un ReCT						// Structure qui va recevoir les coordonnées de la fenêtre trouvée
      
      
      // Récupération du titre de la fenêtre
      sTitre = SysFenTitre(nhwnd)
      
      // On vérifie que la fenêtre est bien visible
      
      si api("USER32","IsWindowVisible",nhwnd) <>0 alors
      	
      	// On vérifie que le titre n'est pas vide
      	
      	si pas sTitre ~= "" ALORS
      		
      		
      		// Récupération des coordonnées de la fenêtre
      		api("USER32","GetWindowRect",nhwnd,&rRectangle)
      		
      		// Vérification des coordonnées de la fenetre
      		// Si elles sont égales les unes aux autres, la fenetre fait 0 x 0 pixels
      		// Donc on ne l'affiche pas
      		
      		si (rRectangle:nHaut - rRectangle:nBas)=0 ou (rRectangle:nGauche - rRectangle:nDroite)= 0 ALORS
      			//On renvoie vrai pour que la fonction d'enumération des fenêtres continue
      			renvoyer vrai
      		FIN
      		
      		si sTitre <> "Program Manager" alors
      			
      			
      			// On récupère le process associé à cette fenetre
      			API("USER32" ,"GetWindowThreadProcessId",nhwnd,&nProcessID)
      			
      			// On récupère l'objet .NET Process
      			MonProcess = process::GetProcessById(nProcessID)
      			
      			// On fait une recherche à l'identique dans la table sur le titre de la fenêtre
      			nLigne = tablecherche(nomappli,sTitre,vrai)
      			
      			// Si la ligne n'est pas trouvée, on l'ajoute
      			si nLigne <> -1  ALORs
      				
      				// Si le handle de fenêtre est égal
      				si fenhandle[nLigne] = nhwnd ALORS
      						
      									
      					// Utilisation de la propriété .NET Responding qui permet de savoir si une application repond
      					// et ajout de la ligne correspondant dans la table				
      					si MonProcess:Responding = vrai alors		
      						tablemodifieligne(tableappli,nLigne,tableappli.IMG[nLigne], " " + sTitre,<§$0006§>,tableappli.ID[nLigne],nhwnd,vrai)
      					sinon
      						tablemodifieligne(tableappli,nLigne,tableappli.IMG[nLigne], " " +sTitre,<§$0007§>,tableappli.ID[nLigne],nhwnd,vrai)
      					fin
      					
      				sinon
      				
      					
      					// récupération de l'icone de l'executable
      					// On récupère le module principal de l'application grâce à la propriété .NET MainModule
      					quand exception dans 
      						MonModule = MonProcess:MainModule
      						// On récupère le chemin complet de l'executable grâce à la propriété .NET FileName
      						sNomExe = MonModule:FileName
      						
      						// Appel d'api pour récupérer le handle de l'icone de l'executable
      						nHndIcone = api("SHELL32","ExtractIconA",SysInstance(),sNomExe,0)
      						
      						// Si le handle de l'icone est non null
      						si nHndIcone <> null ALORS
      							// Dessine l'icone dans le champ image1
      							DessineIcone(nHndIcone,image1..nom)
      						sinon
      							// on met l'image à vide
      							image1=""		
      						fin
      						// Création d'un fichier temporaire
      						sFichierTmp = fFichierTemp() + ".png"
      						// On récupère le repertoire des fichiers temporaires crées
      						sreptemp = fExtraitChemin(sFichierTmp,fDisque+fRépertoire)
      						// on sauvegarde le fichier au format png
      						dSauveImageGIF(Image1,sFichierTmp)					
      					faire
      						sFichierTmp = ""
      					fin
      					
      					// Utilisation de la propriété .NET Responding qui permet de savoir si une application repond
      					// et ajout de la ligne correspondant dans la table
      					
      					si MonProcess:Responding = vrai alors		
      						tableajouteligne(tableappli,sFichierTmp, " " +sTitre,<§$0006§>,nProcessID,nhwnd,vrai)
      					sinon
      						tableajouteligne(tableappli,sFichierTmp, " " +sTitre,<§$0007§>,nProcessID,nhwnd,vrai)
      					fin
      				fin
      				
      			sinon
      			
      			
      				
      				// récupération de l'icone de l'executable
      				// On récupère le module principal de l'application grâce à la propriété .NET MainModule
      				quand exception dans 
      					MonModule = MonProcess:MainModule
      					// On récupère le chemin complet de l'executable grâce à la propriété .NET FileName
      					sNomExe = MonModule:FileName
      					
      					// Appel d'api pour récupérer le handle de l'icone de l'executable
      					nHndIcone = api("SHELL32","ExtractIconA",SysInstance(),sNomExe,0)
      					
      					// Si le handle de l'icone est non null
      					si nHndIcone <> null ALORS
      						// Dessine l'icone dans le champ image1
      						DessineIcone(nHndIcone,image1..nom)
      					sinon
      						// on met l'image à vide
      						image1=""		
      					fin
      					// Création d'un fichier temporaire
      					sFichierTmp = fFichierTemp() + ".png"
      					// On récupère le repertoire des fichiers temporaires crées
      					sreptemp = fExtraitChemin(sFichierTmp,fDisque+fRépertoire)
      					// on sauvegarde le fichier au format png
      					dSauveImageGIF(Image1,sFichierTmp)					
      				faire
      					sFichierTmp = ""
      				fin
      				// Utilisation de la propriété .NET Responding qui permet de savoir si une application repond
      				// et ajout de la ligne correspondant dans la table
      				
      				si MonProcess:Responding = vrai alors		
      					tableajouteligne(tableappli,sFichierTmp, " " +sTitre,<§$0006§>,nProcessID,nhwnd,vrai)
      				sinon
      					tableajouteligne(tableappli,sFichierTmp, " " +sTitre,<§$0007§>,nProcessID,nhwnd,vrai)
      				fin
      				
      			FIN
      
      		fin
      	FIN
      fin
      
      // On renvoie Vrai afin que l'API EnumWindows puisse continuer son énumération
      renvoyer vrai
      
     type : 458752
